/* 设置背景颜色 */
.bg-set {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  background-color: #f5f5f5;
  z-index: -1;
}
.DM-head swiper {
  width: 750rpx;
  height: 470rpx;
}
.DM-head swiper image {
  width: 100%;
  height: 100%;
}
.grid-list {
  display: flex;
  flex-wrap: wrap;
}
.grid-item {
  width: 33.3%;
  height: 140px;
  border: 10px solid #f5f5f5;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
  background-color: #fff;
  border-radius: 30px;
}
.click-hover {
  background-color: #d0d0d0;
}
.grid-item image {
  width: 30px;
  height: 30px;
}
.grid-item text {
  margin-top: 8px;
  font-size: 14px;
}